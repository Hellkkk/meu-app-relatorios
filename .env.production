# Configurações de Produção para EC2
NODE_ENV=production
PORT=5000

# MongoDB - Use sua string de conexão do Atlas ou local
MONGODB_URI=mongodb://localhost:27017/relatorios
# OU para Atlas:
# MONGODB_URI=mongodb+srv://usuario:senha@cluster.mongodb.net/relatorios

# JWT Secret - Gere uma chave segura
JWT_SECRET=sua_chave_jwt_super_secreta_aqui_pelo_menos_32_caracteres

# URLs do Frontend
CLIENT_URL=http://seu-ip-ec2:3000
CORS_ORIGIN=http://seu-ip-ec2:3000

# Configurações de Log
LOG_LEVEL=info