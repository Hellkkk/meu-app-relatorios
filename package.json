{
  "name": "meu-app-relatorios",
  "version": "1.0.0",
  "description": "Backend application with Express.js, MongoDB and JWT authentication",
  "main": "server.js",
  "scripts": {
    "prestart": "node -e \"const { execSync } = require('child_process'); try { const commit = execSync('git rev-parse --short HEAD', { encoding: 'utf8' }).trim(); process.env.APP_COMMIT = commit; console.log('APP_COMMIT set to:', commit); } catch (e) { console.log('Could not determine git commit'); }\"",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "vite",
    "client:build": "vite build",
    "client:preview": "vite preview",
    "install:frontend": "npm install",
    "build:frontend": "npm run client:build",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "deploy:amazon": "chmod +x deploy-amazon-linux.sh && ./deploy-amazon-linux.sh",
    "pm2:start": "pm2 start server.js --name relatorios-backend && pm2 start frontend-server.js --name relatorios-frontend",
    "pm2:stop": "pm2 delete all",
    "pm2:restart": "pm2 restart all",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "express",
    "mongodb",
    "jwt",
    "authentication"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@google/generative-ai": "^0.24.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/x-data-grid": "^8.18.0",
    "@qdrant/js-client-rest": "^1.16.1",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^2.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.4.1",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.4",
    "nodemon": "^3.0.1",
    "vite": "^7.1.12"
  }
}
