{
  "name": "meu-app-relatorios",
  "version": "1.0.0",
  "description": "Backend application with Express.js, MongoDB and JWT authentication",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "vite",
    "client:build": "vite build",
    "client:preview": "vite preview",
    "install:frontend": "npm install",
    "build:frontend": "npm run client:build",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "deploy:amazon": "chmod +x deploy-amazon-linux.sh && ./deploy-amazon-linux.sh",
    "pm2:start": "pm2 start server.js --name backend && pm2 serve dist 3000 --name frontend --spa",
    "pm2:stop": "pm2 delete all",
    "pm2:restart": "pm2 restart all",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "express",
    "mongodb",
    "jwt",
    "authentication"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.4",
    "nodemon": "^3.0.1",
    "vite": "^7.1.12"
  }
}
